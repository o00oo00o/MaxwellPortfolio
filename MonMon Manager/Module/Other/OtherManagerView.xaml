<UserControl x:Class="MaxExperiment.Module.Other.OtherManagerView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Grid>
        <DataGrid ItemsSource="{Binding Contacts}" AutoGenerateColumns="False" SelectionMode="Single">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Name2" Binding="{Binding Name}" />
                <DataGridTextColumn Header="Mobile2" Binding="{Binding Mobile}" />
                <DataGridTextColumn Header="E-Mail2" Binding="{Binding Email}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
